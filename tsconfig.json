{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@assets/*": ["assets/*"],
      "@layouts/*": ["layouts/*"],
      "@svgs/*": ["assets/svgs/*"],
      "@icons/*": ["components/icons/*"]
    }
  }
}
